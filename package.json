  "bugs": {
    "url": "https://github.com/DamonOehlman/binary-type/issues"
  },
  "homepage": "https://github.com/DamonOehlman/binary-type",
  "devDependencies": {
    "brfs": "^1.3.0",
    "browserify": "^10.2.4",
    "tape": "^4.0.0",
    "testling": "^1.7.1",
    "travis-multirunner": "^2.5.0"
  },
  "dependencies": {
    "typedarray-to-buffer": "^3.0.1"
  }
